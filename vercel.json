{
    "version": 2,
    
      "env": {
        "DJANGO_ENV": "production"
      },
    "builds": [
      {
        "src": "backend_pistigreen/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "backend_pistigreen/wsgi.py"
      }
    ]
  }
  